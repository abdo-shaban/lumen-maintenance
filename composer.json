{
  "name": "tumichnix/lumen-maintenance",
  "type": "library",
  "description": "Application maintenance mode for the lumen-framework",
  "keywords": ["lumen", "maintenance", "artisan"],
  "license": "MIT",
  "authors": [
    {
      "name": "tumichnix",
      "email": "tumichnix@gmail.com",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": ">=7.1.3"
  },
  "require-dev": {
    "laravel/lumen-framework": "5.6.*",
    "symfony/var-dumper": "^4.0",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "Tumichnix\\Maintenance\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tumichnix\\Tests\\": "tests/"
    }
  },
  "config":{
    "preferred-install": "dist"
  },
  "minimum-stability": "dev"
}